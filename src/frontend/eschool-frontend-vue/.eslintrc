{
  "root": true,
  "extends": [
    "@antfu/eslint-config-vue"
  ],
  "rules": {
    "no-trailing-spaces": "off",
    "semi": ["error", "never"],
    "comma-dangle": ["error", {
      "functions": "never"
    }],
    "brace-style": ["error", "1tbs"],
    "@typescript-eslint/explicit-function-return-type": [
      "error",
      {
      "allowExpressions": true
      }
    ]
  }
}
